{
  "name": "@sat/template",
  "private": true,
  "version": "1.0.0",
  "packageManager": "pnpm@7.27.1",
  "description": "Serves as a template for workspace packages",
  "scripts": {
    "format": "prettier --write --cache .",
    "lint": "TIMING=1 eslint --fix --cache --ext ts src",
    "typecheck": "tsc --noEmit"
  },
  "eslintConfig": {
    "extends": "@sat/eslint-config"
  },
  "devDependencies": {
    "@sat/eslint-config": "workspace:*",
    "@sat/tsconfig": "workspace:*"
  }
}
